    "compatibleVersions": [
      "v0.0.1-alpha.12"
    ],
    "binaries": {
      "linux/amd64": "https://github.com/liftedinit/manifest-ledger/releases/download/v0.0.1-alpha.12/manifest-ledger_0.0.1-alpha.12_linux_amd64.tar.gz"
    },
    "genesis": {
