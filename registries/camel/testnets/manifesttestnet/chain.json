      },
      {
        "address": "https://manifest-beta-rest.liftedinit.tech/",
        "provider": "Lifted Initiative"
      }
    ],
    "grpc": [
      {
        "address": "https://manifest-beta-grpc.liftedinit.tech/",
        "provider": "Lifted Initiative"
      }
    ]
  },
  "explorers": [
    {
